# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012
AWS_PROFILE=your-profile-name

# Calendly Webhook
CALENDLY_WEBHOOK_SECRET=your_calendly_webhook_secret_here

# AWS SNS Phone Number (E.164 format)
ORIGINATION_NUMBER=+12345678900

# Existing SNS Topic ARN (already connected to your phone number)
INCOMING_SMS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:your-existing-topic

# Business Information
BUSINESS_NAME=Your Business Name

# DynamoDB Tables (auto-generated by CloudFormation)
CONVERSATIONS_TABLE=SMSConversations

# Step Functions (auto-generated by CloudFormation)
STATE_MACHINE_ARN=arn:aws:states:us-east-1:123456789012:stateMachine:SMSFollowUpStateMachine

# Optional: Business Notifications
BUSINESS_NOTIFICATION_TOPIC_ARN=
